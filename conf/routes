# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
POST    /citas                      controllers.CitasController.save(request: Request)
PUT     /citas/:id                  controllers.CitasController.update(id: String, request: Request)
DELETE  /citas/:id                  controllers.CitasController.delete(id: String, request: Request)
